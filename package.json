{
  "name": "pleb-bot",
  "version": "1.0.0",
  "description": "Discordbot for plebs",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run-script update-schema && tsc && node dist/main.js",
    "dev": "./node_modules/.bin/nodemon -e ts --exec \"npm run-script start\"",
    "update-schema": "./node_modules/.bin/openapi-typescript https://api.opendota.com/api --output schema.ts",
    "build": "npm run-script update-schema && tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.12",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "openapi-typescript": "^4.0.2"
  }
}
